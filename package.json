    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/register": "^7.22.15",
    "@types/babel__core": "^7.20.2",
    "@types/bunyan": "^1.8.9",
    "@types/cookie-parser": "^1.4.1",
